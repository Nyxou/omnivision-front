{
    "presets": ["@babel/env", "@babel/react", "@babel/flow"],
    "env": {
      "test": {
        "plugins": ["@babel/plugin-transform-runtime"]
      }
    },
    "plugins": [
      "@babel/plugin-transform-flow-strip-types",
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      "@babel/plugin-proposal-class-properties"
    ]
  }
