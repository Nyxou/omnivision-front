[ignore]
.*/node_modules/*.
.*/private/*.
.*/tmp/*.
.*spec.js

[include]

[libs]
flow-typed

[options]
module.ignore_non_literal_requires=true
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnoreNextLine
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.scss
module.name_mapper='.*\(.s?css\)' -> 'empty/object'
